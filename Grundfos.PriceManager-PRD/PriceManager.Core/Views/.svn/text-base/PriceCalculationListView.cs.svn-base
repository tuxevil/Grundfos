using System;
using System.Collections.Generic;
using System.Text;

namespace PriceManager.Core
{
    public class PriceCalculationListView
    {
        private int iD;
        private string categoryName = "";
        private string providerName = "";
        private string mainFormula = "";
        private string secFormula = "";
        private string priority;

        public virtual int ID
        {
            get { return iD; }
            set { iD = value; }
        }

        public virtual string CategoryName
        {
            get { return categoryName; }
            set { categoryName = value; }
        }

        public virtual string ProviderName
        {
            get { return providerName; }
            set { providerName = value; }
        }

        public virtual string MainFormula
        {
            get { return mainFormula; }
            set { mainFormula = value; }
        }

        public virtual string SecFormula
        {
            get { return secFormula; }
            set { secFormula = value; }
        }

        public virtual string Priority
        {
            get { return priority; }
            set { priority = value; }
        }
    }
}
