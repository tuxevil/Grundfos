using System;
using System.Collections.Generic;
using System.Text;

namespace PartnerNet.Domain.Helpers
{
    public class CustomerHelper
    {
        private string name;
        private string code;

        public string Name
        {
            get { return name; }
            set { name = value; }
        }

        public string Code
        {
            get { return code; }
            set { code = value; }
        }

        public CustomerHelper(string name, string code)
        {
            this.name = code + " - " + name;
            this.code = code;
        }
    }
}
