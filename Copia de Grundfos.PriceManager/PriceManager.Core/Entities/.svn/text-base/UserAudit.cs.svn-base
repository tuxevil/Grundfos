using System;
using System.Collections.Generic;
using System.Text;

namespace PriceManager.Core
{
    public class UserAudit
    {
        private Guid userID;
        private int action;
        private string coments;
        private IList<Product> products;

        public virtual Guid UserID
        {
            get { return userID; }
            set { userID = value; }
        }

        public virtual int Action
        {
            get { return action; }
            set { action = value; }
        }

        public virtual string Coments
        {
            get { return coments; }
            set { coments = value; }
        }

        public virtual IList<Product> Products
        {
            get { return products; }
            set { products = value; }
        }
    }
}
